#!/usr/bin/tclsh
set PRJ_DIR $env(PRJ_DIR)
source ${PRJ_DIR}/design/design_conf/design_def.tcl

set fp [open "strmout_template" w]
puts $fp "library                            \"${TOP_MODULE}_lib\""
puts $fp "strmFile                           \"${TOP_MODULE}_full.gds\""
puts $fp "strmVersion                        \"5\""
puts $fp "runDir                             \".\""
puts $fp "topCell                            \"${TOP_MODULE}\""
puts $fp "view                               \"layout\""
puts $fp "logFile                            \"${TOP_MODULE}_strmout.log\""
puts $fp "summaryFile                        \"\""
puts $fp "techLib                            \"${virtuoso_techno_lib}\""
puts $fp "#hierDepth                         \"32767\""
puts $fp "layerMap                           \"\""
puts $fp "labelMap                           \"\""
puts $fp "labelDepth                         \"1\""
puts $fp "#replaceBusBitChar"
puts $fp "cellMap                            \"\""
puts $fp "fontMap                            \"\""
puts $fp "propMap                            \"\""
puts $fp "objectMap                          \"\""
puts $fp "viaMap                             \"\""
puts $fp "#viaCutArefThreshold               \"0\""
puts $fp "userSkillFile                      \"\""
puts $fp "refLibList                         \"\""
puts $fp "#arrayInstToScalar"
puts $fp "cellNamePrefix                     \"\""
puts $fp "cellNameSuffix                     \"\""
puts $fp "case                               \"preserve\""
puts $fp "labelCase                          \"preserve\""
puts $fp "#ignoreLines"
puts $fp "#noOutputTextDisplays"
puts $fp "#noOutputUnplacedInst"
puts $fp "convertDot                         \"node\""
puts $fp "convertPin                         \"geometry\""
puts $fp "pinAttNum                          \"0\""
puts $fp "#pathToPolygon"
puts $fp "#propValueOnly"
puts $fp "#rectToBox"
puts $fp "#respectGDSIINameLimit"
puts $fp "#flattenPcells"
puts $fp "#flattenVias"
puts $fp "#doNotPreservePcellPins"
puts $fp "#snapToGrid"
puts $fp "#dbuPerUU                          \"0\""
puts $fp "#reportPrecisionLoss"
puts $fp "#noObjectProp"
puts $fp "#ignorePcellEvalFail"
puts $fp "#mergePathSegsToPath"
puts $fp "#noConvertHalfWidthPath"
puts $fp "#checkPolygon"
puts $fp "#backupGdsLogFiles"
puts $fp "maxVertices                        \"200\""
puts $fp "#strmTextNS                        \"cdba\""
puts $fp "#templateFile"
puts $fp "cellListFile                       \"\""
puts $fp "outputDir                          \".\""
puts $fp "noInfo                             \"\""
puts $fp "noWarn                             \"20 156 246\""
puts $fp "warnToErr                          \"\""
puts $fp "#donutNumSides                     \"64\""
puts $fp "#ellipseNumSides                   \"64\""
puts $fp "#wildCardInCellMap"
